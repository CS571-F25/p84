						"index": {
							"byteEnd": 72,
							"byteStart": 64
						}
					},
					{
						"features": [
							{
								"$type": "com.deckbelcher.richtext.facet#cardRef",
								"ref": {
									"oracleUri": "oracle:a7c69802-99c4-4778-934e-cc09ba58c267",
